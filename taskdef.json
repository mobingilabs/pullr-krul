{
    "family": "%TASK_NAME%",
    "containerDefinitions": [
        {
            "image": "%IMAGE_URI%:%BUILD_TAG%",
            "name": "%CONTAINER_NAME%",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0
                }
            ]
        }
    ]
}
